{
  "name": "node-ts-express-prisma-template",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node --enable-source-maps dist/index.js",
    "build:ts": "tsc --sourceMap",
    "build": "run-s prisma:generate build:ts copy:prisma-generated",
    "dev": "dotenv -- tsc-watch --sourceMap --onsuccess \"run-s dev:start\"",
    "dev:start": "run-s copy:prisma-generated start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "verify": "run-s lint test test:coverage",
    "prisma:generate": "prisma generate",
    "copy:prisma-generated": "node ./scripts/copy-prisma-generated.mjs",
    "migration:create": "prisma migrate dev --create-only",
    "migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tsconfig/node20": "^20.1.9",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.1",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.2",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "jsonc-eslint-parser": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "express": "^5.2.1",
    "joi": "^18.0.2",
    "winston": "^3.19.0"
  }
}
